{
  "name": "@retailcrm/embed-ui-v1-components",
  "type": "module",
  "version": "0.0.0",
  "license": "MIT",
  "author": "RetailDriverLLC <integration@retailcrm.ru>",
  "repository": "git@github.com:retailcrm/embed-ui.git",
  "exports": {
    ".": {
      "require": "./index.cjs",
      "import": "./index.mjs",
      "default": "./index.mjs"
    },
    "./host": {
      "types": "./dist/host.d.ts",
      "require": "./dist/host.cjs",
      "import": "./dist/host.mjs",
      "default": "./dist/host.mjs"
    },
    "./remote": {
      "types": "./dist/remote.d.ts",
      "require": "./dist/remote.cjs",
      "import": "./dist/remote.mjs",
      "default": "./dist/remote.mjs"
    },
    "./assets/*": "./assets/*",
    "./dist/*": "./dist/*"
  },
  "typesVersions": {
    "*": {
      "host": [
        "./dist/host.d.ts"
      ],
      "remote": [
        "./dist/remote.d.ts"
      ]
    }
  },
  "files": [
    "assets",
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "yarn build:host && yarn build:remote",
    "build:host": "vite build -c ./vite.config.host.ts",
    "build:remote": "vite build -c ./vite.config.remote.ts",
    "storybook:build": "storybook build -c storybook",
    "storybook:serve": "storybook dev -c storybook -p 6006 --no-open"
  },
  "peerDependencies": {
    "vue": "^3.5"
  },
  "dependencies": {
    "@omnicajs/vue-remote": "^0.2.1",
    "@remote-ui/rpc": "^1.4.5"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^8.4.4",
    "@storybook/addon-essentials": "^8.4.4",
    "@storybook/addon-interactions": "^8.4.4",
    "@storybook/addon-links": "^8.4.4",
    "@storybook/addon-themes": "^8.4.4",
    "@storybook/blocks": "^8.4.4",
    "@storybook/builder-vite": "^8.4.4",
    "@storybook/manager-api": "^8.4.4",
    "@storybook/test": "^8.4.4",
    "@storybook/theming": "^8.4.4",
    "@storybook/vue3": "^8.4.4",
    "@storybook/vue3-vite": "^8.4.4",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/compiler-sfc": "^3.5.12",
    "less": "^4.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "storybook": "^8.4.4",
    "tsx": "^4.19.2",
    "uuid": "^11.0.3",
    "vite": "^5.4.11",
    "vite-plugin-dts": "^4.3.0",
    "vite-svg-loader": "^5.1.0",
    "vue": "^3.5.12",
    "vue3-perfect-scrollbar": "^1.6.0"
  }
}
